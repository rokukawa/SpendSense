{
  "name": "spend-sense",
  "version": "1.0.0",
  "private": true,
  "main": "dist/bin/www.js",
  "scripts": {
    "start": "nodemon",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist/bin/www",
    "dev": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "clean": "rimraf dist",
    "transpile": "babel ./server --out-dir dist --copy-files",
    "lint": "eslint ./server",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "dependencies": {
    "axios": "^1.4.0",
    "axios-cookiejar-support": "^4.0.6",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.1.4",
    "express": "~4.16.1",
    "express-handlebars": "^7.0.7",
    "express-session": "^1.17.3",
    "helmet": "^3.22.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.9.1",
    "mustache-express": "^1.3.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.0",
    "tough-cookie": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.9.0",
    "babel-plugin-inline-dotenv": "^1.5.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^25.5.4",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "supertest": "^5.0.0"
  }
}
